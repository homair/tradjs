{
  "name": "tradjs",
  "version": "2.0.0",
  "description": "Homair i18n backend in Node.js",
  "main": "boot.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/homair/tradjs.git"
  },
  "author": "Homair team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/homair/tradjs/issues"
  },
  "homepage": "https://github.com/homair/tradjs#readme",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "dev": "webpack --watch --progress --config webpack.config.js",
    "heroku-postbuild": "webpack",
    "test": "NODE_ENV=test mocha",
    "start": "nodemon boot.js",
    "server": "NODE_ENV=dev PORT=3006 nodemon boot.js",
    "install-mac": "npm i fsevents"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "eslintConfig": {
    "globals": {
      "pageData": true,
      "$": true
    }
  },
  "pre-commit": [
    "test"
  ],
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "dependencies": {
    "@babel/cli": "7.0.x",
    "@babel/core": "7.0.x",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.1.0",
    "@babel/preset-env": "7.0.x",
    "@babel/register": "7.0.x",
    "async": "^2.6.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "body-parser": "^1.18.3",
    "bootbox": "^4.4.0",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^1.0.0",
    "colors": "^1.3.2",
    "compression": "^1.7.3",
    "compression-webpack-plugin": "^2.0.0",
    "cookie-session": "^2.0.0-beta.3",
    "css-loader": "^1.0.1",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "floatthead": "^2.1.2",
    "font-awesome": "^4.7.0",
    "hard-source-webpack-plugin": "^0.12.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.4.5",
    "mongodb": "^3.1.10",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "plugin": "^0.3.3",
    "popper.js": "^1.14.6",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.3",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "tether": "^1.4.5",
    "webpack": "^4.27.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "supertest": "^3.3.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  }
}
