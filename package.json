{
  "name": "tradjs",
  "version": "1.0.0",
  "description": "Homair i18n backend in Node.js",
  "main": "boot.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/homair/tradjs.git"
  },
  "author": "Homair team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/homair/tradjs/issues"
  },
  "homepage": "https://github.com/homair/tradjs#readme",
  "scripts": {
    "build": "NODE_ENV=dev webpack",
    "dev:client": "webpack --watch",
    "dev:server": "nodemon boot.js",
    "dev:test": "npm run test -- --watch",
    "start": "node boot.js",
    "test": "NODE_ENV=test mocha",
    "install": "npm i -g webpack"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "standard": {
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "body-parser": "^1.15.2",
    "bootbox": "^4.4.0",
    "bootstrap": "^4.0.0-alpha.2",
    "bootstrap-material-design": "^0.5.10",
    "colour": "^0.7.1",
    "compression": "^1.6.2",
    "css-loader": "^0.23.1",
    "csurf": "^1.9.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "floatthead": "^1.4.2",
    "hjs-webpack": "^8.3.0",
    "jquery": "^3.0.0",
    "jquery-modal": "^0.7.0",
    "jquery-ui": "^1.12.0",
    "lodash": "^4.2.0",
    "mongodb": "^2.1.18",
    "morgan": "^1.7.0",
    "postcss-loader": "^0.9.1",
    "pug": "^2.0.0-beta3",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "tether": "^1.3.4"
  },
  "devDependencies": {
    "ab-tool": "^0.2.0",
    "babel-eslint": "^6.1.2",
    "chai": "^3.5.0",
    "mocha": "^2.5.3",
    "mysql": "^2.11.1",
    "node-sass": "^3.8.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.2.0",
    "unfluff": "^1.1.0"
  }
