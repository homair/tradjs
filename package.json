  "babel": {
    "presets": [
      "env",
      "stage-2"
    ]
  },
  "standard": {
    "env": {
      "node": true,
      "mocha": true
    }
  },
  "pre-commit": [
    "test"
  ],
  "engines": {
    "node": "8.x"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootbox": "^4.4.0",
    "bootstrap": "^4.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "colors": "^1.3.0",
    "compression": "^1.7.2",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "floatthead": "^2.1.2",
    "font-awesome": "^4.7.0",
    "hard-source-webpack-plugin": "^0.7.5",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.0",
    "mongodb": "^3.0.10",
    "morgan": "^1.9.0",
    "node-sass": "^4.9.0",
    "plugin": "^0.3.3",
    "popper.js": "^1.14.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "pug": "^2.0.3",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "tether": "^1.4.4",
    "webpack": "^4.10.2",
    "webpack-dashboard": "^2.0.0",
    "winston": "^2.4.2",
    "zopfli-webpack-plugin": "^0.1.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0",
    "webpack-cli": "^3.0.2",
    "webpack-merge": "^4.1.2"
